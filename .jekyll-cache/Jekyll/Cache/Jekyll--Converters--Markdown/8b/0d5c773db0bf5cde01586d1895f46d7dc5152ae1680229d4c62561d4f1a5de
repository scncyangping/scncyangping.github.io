public abstract class Staff {

    public String name;
    public int kpi;// å‘˜å·¥KPI

    public Staff(String name) {
        this.name = name;
        kpi = new Random().nextInt(10);
    }
    // æ ¸å¿ƒæ–¹æ³•ï¼Œæ¥å—Visitorçš„è®¿é—®
    public abstract void accept(Visitor visitor);
}

// å·¥ç¨‹å¸ˆ
public class Engineer extends Staff {

    public Engineer(String name) {
        super(name);
    }

    @Override
    public void accept(Visitor visitor) {
        visitor.visit(this);
    }
    // å·¥ç¨‹å¸ˆä¸€å¹´çš„ä»£ç æ•°é‡
    public int getCodeLines() {
        return new Random().nextInt(10 * 10000);
    }
}


// ç»ç†
public class Manager extends Staff {

    public Manager(String name) {
        super(name);
    }

    @Override
    public void accept(Visitor visitor) {
        visitor.visit(this);
    }
    // ä¸€å¹´åšçš„äº§å“æ•°é‡
    public int getProducts() {
        return new Random().nextInt(10);
    }
}

// å‘˜å·¥ä¸šåŠ¡æŠ¥è¡¨ç±»
public class BusinessReport {

    private List&lt;Staff&gt; mStaffs = new LinkedList&lt;&gt;();

    public BusinessReport() {
        mStaffs.add(new Manager("ç»ç†-A"));
        mStaffs.add(new Engineer("å·¥ç¨‹å¸ˆ-A"));
        mStaffs.add(new Engineer("å·¥ç¨‹å¸ˆ-B"));
        mStaffs.add(new Engineer("å·¥ç¨‹å¸ˆ-C"));
        mStaffs.add(new Manager("ç»ç†-B"));
        mStaffs.add(new Engineer("å·¥ç¨‹å¸ˆ-D"));
    }

    /**
     * ä¸ºè®¿é—®è€…å±•ç¤ºæŠ¥è¡¨
     * @param visitor å…¬å¸é«˜å±‚ï¼Œå¦‚CEOã€CTO
     */
    public void showReport(Visitor visitor) {
        for (Staff staff : mStaffs) {
            staff.accept(visitor);
        }
    }
}


public interface Visitor {

    // è®¿é—®å·¥ç¨‹å¸ˆç±»å‹
    void visit(Engineer engineer);

    // è®¿é—®ç»ç†ç±»å‹
    void visit(Manager manager);
}

// CEOè®¿é—®è€…
public class CEOVisitor implements Visitor {
    @Override
    public void visit(Engineer engineer) {
        System.out.println("å·¥ç¨‹å¸ˆ: " + engineer.name + ", KPI: " + engineer.kpi);
    }

    @Override
    public void visit(Manager manager) {
        System.out.println("ç»ç†: " + manager.name + ", KPI: " + manager.kpi +
                ", æ–°äº§å“æ•°é‡: " + manager.getProducts());
    }
}

public class CTOVisitor implements Visitor {
    @Override
    public void visit(Engineer engineer) {
        System.out.println("å·¥ç¨‹å¸ˆ: " + engineer.name + ", ä»£ç è¡Œæ•°: " + engineer.getCodeLines());
    }

    @Override
    public void visit(Manager manager) {
        System.out.println("ç»ç†: " + manager.name + ", äº§å“æ•°é‡: " + manager.getProducts());
    }
}

public class Client {

    public static void main(String[] args) {
        // æ„å»ºæŠ¥è¡¨
        BusinessReport report = new BusinessReport();
        System.out.println("=========== CEOçœ‹æŠ¥è¡¨ ===========");
        report.showReport(new CEOVisitor());
        System.out.println("=========== CTOçœ‹æŠ¥è¡¨ ===========");
        report.showReport(new CTOVisitor());
    }
}
