		<span class="n">return</span> <span class="n">lNode</span>
	<span class="p">}</span>
	<span class="n">node</span><span class="p">.</span><span class="n">right</span> <span class="p">=</span> <span class="n">removeMax</span><span class="p">(</span><span class="n">node</span><span class="p">.</span><span class="n">right</span><span class="p">)</span>
	<span class="n">return</span> <span class="n">node</span>
